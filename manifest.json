{
    "manifest_version": 3,
    "name": "LeShrine",
    "version": "1.0",
    "description": "Replace ads with images of LeBron James.",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png",
      "128": "icon128.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    }
  }
  