{
    "manifest_version": 3,
    "name": "LeShrine",
    "version": "1.0",
    "description": "Replace ads with images of LeBron James.",
    "permissions": ["activeTab", "scripting", "storage", "declarativeNetRequest"],
    "host_permissions": ["<all_urls>"],
    "declarative_net_request":{
      "rule_resources": [
        {
          "id": "block-ads",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    },
    "web_accessible_resources": [
      {
        "resources": [
          "/media/image/blockedByJames.jpg",
          "media/audio/blockedByJames.mp3"
        ],
        "matches": ["<all_urls>"]
      }
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "/media/image/king16.png",
      "32": "/media/image/king32.png",
      "48": "/media/image/king48.png",
      "128": "/media/image/king128.png"
    },
    "chrome_url_overrides": {
      "newtab": "src/html/index.html"
    }
  }
  